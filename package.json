{
  "name": "ismax.dashboard",
  "version": "1.0.0",
  "description": "Dashboard for ismax API",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run webpack",
    "server": "better-npm-run server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiondlph/ismax.dashboard.git"
  },
  "keywords": [
    "ismax",
    "dashboard"
  ],
  "author": "Shukhrat <shukhrat@ismax.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiondlph/ismax.dashboard/issues"
  },
  "homepage": "https://github.com/xiondlph/ismax.dashboard#readme",
  "betterScripts": {
    "server": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "webpack": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-function-bind"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "cascade": false
      }
    }
  },
  "browserslist": [
    "> 5%",
    "IE > 10"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.7",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
